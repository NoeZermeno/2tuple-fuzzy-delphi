}

.clearfooter{
   height: 10px;
   clear: both;
}

footer {
  height: 50px;
  width: 100%;
  background:#4F4C4D;
  padding:10px 0;
  margin-top: 15px;
  color:#fff;
  display: inline-block;
}

footer p {
  text-align:center;
  font-size: 12px;
}



.iconos{
	color: white;
	margin-right: 30px;

	display: inline-block;
	font-size: 25px;
	float: right;
}


.iconos_redes{
	background-color: #fff;
	height: 50px;
}
